<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.example.dep</groupId>
  <artifactId>ci-sample-java-maven</artifactId>
  <packaging>jar</packaging>
  <version>1.0</version>
  <name>ci-sample-java-maven</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
        <id>gitlab</id>
        <name>${env.MAVEN_RELEASE_REPO}</name>
        <url>${env.MAVEN_RELEASE_URL}</url>
    </repository>
    <snapshotRepository>
        <id>gitlab</id>
        <name>${env.MAVEN_SNAPSHOT_REPO}</name>
        <url>${env.MAVEN_SNAPSHOT_URL}</url>
    </snapshotRepository>
  </distributionManagement>
</project>
